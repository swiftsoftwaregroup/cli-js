{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
      "include": ["cli.js", "README.md"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "destination": "./docs/",
      "readme": "./README.md",
      "recurse": true,
      "template": "node_modules/docdash"
    }
  }